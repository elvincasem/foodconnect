<?xml version="1.0" encoding="UTF-8" ?>
    
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.foodconnect.launion"
        versionCode = "1" 
        version     = "1.0.0" >
        <name>Food Connect</name>
        <description>
            Find the ultimate dining experience in La Union.
        </description>
        <author email="djmontemayor78@gmail.com" href="http://foodconnect.com">
